package com.campusFacilities.www.repository;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.campusFacilities.www.model.Transport.Bus;
import com.campusFacilities.www.model.Transport.BusPass;
import com.campusFacilities.www.model.Transport.RouteWay;


@Repository
public interface TransportRepository extends JpaRepository<Bus, Long>,
                                            JpaRepository<BusPass, Long>,
                                            JpaRepository<RouteWay, Long> {
}
