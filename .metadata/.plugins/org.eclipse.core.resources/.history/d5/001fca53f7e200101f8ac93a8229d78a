package com.campusFacilities.www.service.Imp;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import com.campusFacilities.www.repository.TransportRepository;

@Service
public class TransportService {
	
	@Autowired
	private TransportRepository transport;  

	
	public Transport addBusNum(BusNum busnum)
	{
		return TransportRepository.save(busnum)
}

}
